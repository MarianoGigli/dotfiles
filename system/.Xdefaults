! special colors
*.foreground:  #b9b9b9
*.background:  #101010
*.cursorColor:  #b9b9b9
! black
*.color0:      #101010
*.color8:      #525252
! red
*.color1:      #7c7c7c
*.color9:      #7c7c7c
! green
*.color2:      #8e8e8e
*.color10:     #8e8e8e
! yellow
*.color3:      #a0a0a0
*.color11:     #a0a0a0
! blue
*.color4:      #686868
*.color12:     #686868
! magenta
*.color5:      #747474
*.color13:     #747474
! cyan
*.color6:      #868686
*.color14:     #868686
! white
*.color7:      #b9b9b9
*.color15:     #f7f7f7

URxvt*scrollBar:         false
URxvt*scrollBar_right:   true

URxvt*transparent:       false

URxvt*depth: 32
URxvt.background: [95]#101010

URxvt.font: xft:Source Code Pro for Powerline:style=Bold:pixelsize=12:minspace=False,xft:Symbola:minspace=False
URxvt.letterSpace: 1

*antialias:  true
*autohint:   true


URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg4
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

URxvt.perl-ext-common: ...,tabbed,...
URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 7
URxvt.tabbed.tab-bg: 9
