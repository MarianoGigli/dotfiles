! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.terminal: roxterm
rofi.color-enabled: true
rofi.font: System San Francisco Display Regular 18
rofi.lines: 3
rofi.eh: 2
rofi.width: 100
rofi.padding: 200
rofi.fullscreen: true
rofi.bw: -1
rofi.separator-style: none
rofi.hide-scrollbar: false
rofi.fake-transparency: true
!                  'background', 'border', 'separator'
rofi.color-window: argb:750039ff, #0039ff, #4F80FF
! State:           'bg',    'fg',    'bgalt', 'hlbg', 'hlfg'
rofi.color-normal: #0039ff, #4f80ff, #0039ff, #4f80ff, #ffffff
rofi.color-active: #0039ff, #68ba50, #0039ff, #4f80ff, #68ba50
rofi.color-urgent: #0039ff, #ff7047, #0039ff, #4f80ff, #ff7047

! ------------------------------------------------------------------------------
! XST configuration and Color theme
! ------------------------------------------------------------------------------
! These options only take effect on startup.
st.termname: st-256color

! The following options options can be reloaded via USR1 signal.
st.font: Source Code Pro for Powerline:style=semibold:pixelsize=14:antialias=true:autohint=true
st.borderpx: 10
! Available cursor values: 2 4 6 7 = █ _ | ☃ ( 1 3 5 are blinking versions)
st.cursorshape: 1
